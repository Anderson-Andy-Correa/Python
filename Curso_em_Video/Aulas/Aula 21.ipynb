   "source": [
    "def fatorial(num=1):\n",
    "    f = 1\n",
    "    for c in range(num, 0 ,-1):\n",
    "        f *= c\n",
    "    return f\n",
    "        \n",
    "n = int(input('Digite um número: '))\n",
    "print(f'O fatorial de {n} é igual a {fatorial(n)}')"
   ]
